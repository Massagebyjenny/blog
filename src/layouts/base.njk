  font-weight: 650;
}

.divider{
  height: 1px;
  background: var(--line);
  margin: 18px 0;
}

.ul{
  margin: 10px 0 0;
  padding-left: 18px;
  color: var(--muted);
}
.ul li{ margin: 8px 0; }

